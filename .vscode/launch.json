    "version": "0.2.0",
    "configurations": [
            {
            "type": "node",
            "request": "launch",
            "name": "Working thru node",
            "program": "${workspaceFolder}/node_modules/.bin/protractor",
            "args": [
            "./protractor.conf.js",
            "--directConnect=true"
            ],
            "env": {
            "LOG_LEVEL": "ALL"
            },
            "protocol": "inspector",
            "sourceMaps": true,
            "console": "internalConsole",
            "outputCapture": "std"
            }
    ]
}